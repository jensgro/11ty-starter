{
  "name": "11ty-starter",
  "version": "1.0.0",
  "description": "Minimal starter-kit for 11ty.",
  "scripts": {
    "build": "npm-run-all eleventy:default css",
    "clean": "rm -rf _site",
    "watch": "eleventy --watch",
    "serve": "eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "css": "sass src/assets/scss/:_site/css/",
    "dev": "npm-run-all clean styles:dev build --parallel watch serve"
  },
  "keywords": [],
  "author": {
    "name": "Jens Grochtdreis",
    "email": "jens@grochtdreis.de",
    "url": "https://grochtdreis.de"
  },
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "autoprefixer": "^10.4.13",
    "bootstrap": "^5.2.2",
    "gulp": "^4.0.2",
    "gulp-sass": "^5.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0",
    "sass": "^1.56.1"
  }
}
